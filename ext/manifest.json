{
    "manifest_version": 3,
    "name": "parser job posts Linkedin",
    "description": "Расширение для парсинга job post'ов из Linkedin",
    "version": "1.1",
    "icons": {
	"128": "images/jobs.png"
    },
    "background":{
	"service_worker":"background.js"
    },

    "permissions": [
	"unlimitedStorage",
	"activeTab",
	"scripting"
    ],
    "host_permissions":[
	"http://127.0.0.1:7700/*",
	"http://localhost:7700/*"
    ],

    "action": {
	"default_popup":"popup.html"
    }
}
